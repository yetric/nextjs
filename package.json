{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3737",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write {cli,client,lib,pages,server,tests}/**/*.{js,ts,jsx,tsx,md,scss,css,json}",
    "format-check": "prettier --check {cli,client,lib,pages,server,tests}/**/*.{js,ts,jsx,tsx,md,scss,css,json}",
    "node-check": "node cli/check_node_version.js",
    "postinstall": "npm run node-check"
  },
  "dependencies": {
    "next": "9.5.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "semver": "^7.3.2"
  },
  "devDependencies": {
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.46",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "typescript": "^3.9.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,md}": "prettier --write"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=14.6.0"
  }
}
