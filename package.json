{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3737",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write {cli,client,lib,pages,server,tests}/**/*.{js,ts,jsx,tsx,md,scss,css,json}",
    "format-check": "prettier --check {cli,client,lib,pages,server,tests}/**/*.{js,ts,jsx,tsx,md,scss,css,json}",
    "node-check": "node cli/check_node_version.js",
    "tslint": "tslint -p . -c tslint.json",
    "deploy": "npm run format-check && npm run tslint && npm build",
    "postinstall": "npm run node-check"
  },
  "dependencies": {
    "next": "9.5.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "semver": "^7.3.2"
  },
  "devDependencies": {
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.46",
    "babel-plugin-module-resolver": "^4.0.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "tslint": "^6.1.3",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-react-hooks": "^2.2.2",
    "typescript": "^3.9.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,md}": "prettier --write"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=14.6.0"
  }
}
